      <item ident="g25ead5d65f9400c1d197407ea40a512b" title="Question">
        <itemmetadata>
          <qtimetadata>
            <qtimetadatafield>
              <fieldlabel>cc_profile</fieldlabel>
              <fieldentry>cc.multiple_choice.v0p1</fieldentry>
            </qtimetadatafield>
          </qtimetadata>
        </itemmetadata>
        <presentation>
          <material>
            <mattext texttype="text/html">&lt;div&gt;&lt;span&gt;Which is better for finding shortest paths on a cyclic graph?&lt;/span&gt;&lt;/div&gt;</mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="5542">
                <material>
                  <mattext texttype="text/plain">Breadth-First Traversal</mattext>
                </material>
              </response_label>
              <response_label ident="4290">
                <material>
                  <mattext texttype="text/plain">Depth-First Traversal</mattext>
                </material>
              </response_label>
              <response_label ident="4973">
                <material>
                  <mattext texttype="text/plain">Level-First Traversal</mattext>
                </material>
              </response_label>
              <response_label ident="8575">
                <material>
                  <mattext texttype="text/plain">Pre-order Traversal</mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar maxvalue="100" minvalue="0" varname="SCORE" vartype="Decimal"/>
          </outcomes>
          <respcondition continue="Yes">
            <conditionvar>
              <varequal respident="response1">4290</varequal>
            </conditionvar>
            <displayfeedback feedbacktype="Response" linkrefid="4290_fb"/>
          </respcondition>
          <respcondition continue="Yes">
            <conditionvar>
              <varequal respident="response1">4973</varequal>
            </conditionvar>
            <displayfeedback feedbacktype="Response" linkrefid="4973_fb"/>
          </respcondition>
          <respcondition continue="Yes">
            <conditionvar>
              <varequal respident="response1">8575</varequal>
            </conditionvar>
            <displayfeedback feedbacktype="Response" linkrefid="8575_fb"/>
          </respcondition>
          <respcondition continue="No">
            <conditionvar>
              <varequal respident="response1">5542</varequal>
            </conditionvar>
            <setvar action="Set" varname="SCORE">100</setvar>
          </respcondition>
        </resprocessing>
        <itemfeedback ident="4290_fb">
          <flow_mat>
            <material>
              <mattext texttype="text/html">&lt;i&gt;&lt;span&gt;Since Depth-First Traversal dives deep, it might find a round-about longer path than the shortest.&lt;/span&gt;&lt;/i&gt;</mattext>
            </material>
          </flow_mat>
        </itemfeedback>
        <itemfeedback ident="4973_fb">
          <flow_mat>
            <material>
              <mattext texttype="text/html">&lt;p&gt;&lt;i&gt;&lt;/i&gt;&lt;/p&gt;
&lt;p&gt;&lt;i&gt;&lt;span&gt;Level-First is another name for Breadth-First that applies only to trees. Since this is a cyclic graph, it cannot be a tree.&lt;/span&gt;&lt;/i&gt;&lt;/p&gt;
&lt;p&gt;&lt;i&gt;&lt;/i&gt;&lt;/p&gt;</mattext>
            </material>
          </flow_mat>
        </itemfeedback>
        <itemfeedback ident="8575_fb">
          <flow_mat>
            <material>
              <mattext texttype="text/html">&lt;p&gt;&lt;i&gt;&lt;/i&gt;&lt;/p&gt;
&lt;p&gt;&lt;i&gt;&lt;span&gt;This is a type of depth-first traversal that only applies to Binary Trees&lt;/span&gt;&lt;/i&gt;&lt;i&gt;&lt;/i&gt;&lt;/p&gt;</mattext>
            </material>
          </flow_mat>
        </itemfeedback>
      </item>
      <item ident="ga6389fa0aaaa27b8ef65bfbf60982be6" title="Question">
