                </material>
              </response_label>
              <response_label ident="2388">
                <material>
                  <mattext texttype="text/plain">Breadth-First Traversal</mattext>
                </material>
              </response_label>
              <response_label ident="619">
                <material>
                  <mattext texttype="text/plain">Level-First Traversal</mattext>
                </material>
              </response_label>
            </render_choice>
