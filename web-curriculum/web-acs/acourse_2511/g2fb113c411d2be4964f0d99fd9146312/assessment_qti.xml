        <presentation>
          <material>
            <mattext texttype="text/html">&lt;div&gt;&lt;span&gt;Which type of traversal spreads outward from the starting point on a cyclic graph at an even pace?&lt;/span&gt;&lt;/div&gt;</mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="2613">
                <material>
                  <mattext texttype="text/plain">Breadth-First Traversal</mattext>
                </material>
              </response_label>
              <response_label ident="86">
                <material>
                  <mattext texttype="text/plain">Depth-First Traversal</mattext>
                </material>
              </response_label>
              <response_label ident="9552">
                <material>
                  <mattext texttype="text/plain">Level-First Traversal</mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar maxvalue="100" minvalue="0" varname="SCORE" vartype="Decimal"/>
          </outcomes>
          <respcondition continue="Yes">
            <conditionvar>
              <varequal respident="response1">86</varequal>
            </conditionvar>
            <displayfeedback feedbacktype="Response" linkrefid="86_fb"/>
          </respcondition>
          <respcondition continue="Yes">
            <conditionvar>
              <varequal respident="response1">9552</varequal>
            </conditionvar>
            <displayfeedback feedbacktype="Response" linkrefid="9552_fb"/>
          </respcondition>
          <respcondition continue="No">
            <conditionvar>
              <varequal respident="response1">2613</varequal>
            </conditionvar>
            <setvar action="Set" varname="SCORE">100</setvar>
          </respcondition>
        </resprocessing>
        <itemfeedback ident="86_fb">
          <flow_mat>
            <material>
              <mattext texttype="text/html">&lt;p&gt;&lt;i&gt;&lt;/i&gt;&lt;/p&gt;
&lt;p&gt;&lt;i&gt;&lt;span&gt;Depth-First dives as deeply as possible into the graph before backtracking minimum distance to keep exploring.&lt;/span&gt;&lt;/i&gt;&lt;/p&gt;
&lt;p&gt;&lt;i&gt;&lt;/i&gt;&lt;/p&gt;</mattext>
            </material>
          </flow_mat>
        </itemfeedback>
        <itemfeedback ident="9552_fb">
          <flow_mat>
            <material>
              <mattext texttype="text/html">&lt;i&gt;&lt;span&gt;Level-First is another name for Breadth-First that applies only to trees. Since this is a cyclic graph, it cannot be a tree.&lt;/span&gt;&lt;/i&gt;</mattext>
            </material>
          </flow_mat>
        </itemfeedback>
      </item>
