      <item ident="g31b3f47344ea92361ff3eb0c73053173" title="Question">
        <itemmetadata>
          <qtimetadata>
            <qtimetadatafield>
              <fieldlabel>cc_profile</fieldlabel>
              <fieldentry>cc.multiple_choice.v0p1</fieldentry>
            </qtimetadatafield>
          </qtimetadata>
        </itemmetadata>
        <presentation>
          <material>
            <mattext texttype="text/html">&lt;div&gt;&lt;span&gt;What are the characteristics of a binary search tree?&lt;/span&gt;&lt;/div&gt;</mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="5051">
                <material>
                  <mattext texttype="text/plain">All nodes greater than a node are on one side of that node.</mattext>
                </material>
              </response_label>
              <response_label ident="9592">
                <material>
                  <mattext texttype="text/plain">It must be balanced at all times.</mattext>
                </material>
              </response_label>
              <response_label ident="1479">
                <material>
                  <mattext texttype="text/plain">All values in the tree must be 0 or 1.</mattext>
                </material>
              </response_label>
              <response_label ident="5391">
                <material>
                  <mattext texttype="text/plain">You get O(1) search times.</mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar maxvalue="100" minvalue="0" varname="SCORE" vartype="Decimal"/>
          </outcomes>
          <respcondition continue="Yes">
            <conditionvar>
              <varequal respident="response1">9592</varequal>
            </conditionvar>
            <displayfeedback feedbacktype="Response" linkrefid="9592_fb"/>
          </respcondition>
          <respcondition continue="Yes">
            <conditionvar>
              <varequal respident="response1">1479</varequal>
            </conditionvar>
            <displayfeedback feedbacktype="Response" linkrefid="1479_fb"/>
          </respcondition>
          <respcondition continue="Yes">
            <conditionvar>
              <varequal respident="response1">5391</varequal>
            </conditionvar>
            <displayfeedback feedbacktype="Response" linkrefid="5391_fb"/>
          </respcondition>
          <respcondition continue="No">
            <conditionvar>
              <varequal respident="response1">5051</varequal>
            </conditionvar>
            <setvar action="Set" varname="SCORE">100</setvar>
          </respcondition>
        </resprocessing>
        <itemfeedback ident="9592_fb">
          <flow_mat>
            <material>
              <mattext texttype="text/html">&lt;i&gt;&lt;span&gt;Binary Search Trees lose their O(log n) performance if they're unbalanced. But there's no law that says they must be.&lt;/span&gt;&lt;/i&gt;</mattext>
            </material>
          </flow_mat>
        </itemfeedback>
        <itemfeedback ident="1479_fb">
          <flow_mat>
            <material>
              <mattext texttype="text/html">&lt;i&gt;&lt;span&gt;&amp;nbsp;This is not a requirement of any kind of tree.&lt;/span&gt;&lt;/i&gt;</mattext>
            </material>
          </flow_mat>
        </itemfeedback>
        <itemfeedback ident="5391_fb">
          <flow_mat>
            <material>
              <mattext texttype="text/html">&lt;i&gt;&lt;span&gt;Binary Search Trees give O(log n) search times. No types of trees give O(1) search times.&lt;/span&gt;&lt;/i&gt;</mattext>
            </material>
          </flow_mat>
        </itemfeedback>
      </item>
      <item ident="g30a579ea4127a2a7aead86ff934ef56f" title="Question">
        <itemmetadata>
          <qtimetadata>
            <qtimetadatafield>
              <fieldlabel>cc_profile</fieldlabel>
              <fieldentry>cc.multiple_choice.v0p1</fieldentry>
            </qtimetadatafield>
          </qtimetadata>
        </itemmetadata>
        <presentation>
          <material>
            <mattext texttype="text/html">&lt;div&gt;&lt;span&gt;When might you have an O(log n) process?&lt;/span&gt;&lt;/div&gt;</mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="4469">
                <material>
                  <mattext texttype="text/plain">When you halve the potential search space every step</mattext>
                </material>
              </response_label>
              <response_label ident="347">
                <material>
                  <mattext texttype="text/plain">When you double the potential search space every step</mattext>
                </material>
              </response_label>
              <response_label ident="4838">
                <material>
                  <mattext texttype="text/plain">When you have to search every node in the data structure</mattext>
                </material>
              </response_label>
              <response_label ident="3794">
                <material>
                  <mattext texttype="text/plain">When it takes constant time to search for a value</mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar maxvalue="100" minvalue="0" varname="SCORE" vartype="Decimal"/>
          </outcomes>
          <respcondition continue="Yes">
            <conditionvar>
              <varequal respident="response1">347</varequal>
            </conditionvar>
            <displayfeedback feedbacktype="Response" linkrefid="347_fb"/>
          </respcondition>
          <respcondition continue="Yes">
            <conditionvar>
              <varequal respident="response1">4838</varequal>
            </conditionvar>
            <displayfeedback feedbacktype="Response" linkrefid="4838_fb"/>
          </respcondition>
          <respcondition continue="Yes">
            <conditionvar>
              <varequal respident="response1">3794</varequal>
            </conditionvar>
            <displayfeedback feedbacktype="Response" linkrefid="3794_fb"/>
          </respcondition>
          <respcondition continue="No">
            <conditionvar>
              <varequal respident="response1">4469</varequal>
            </conditionvar>
            <setvar action="Set" varname="SCORE">100</setvar>
          </respcondition>
        </resprocessing>
        <itemfeedback ident="347_fb">
          <flow_mat>
            <material>
              <mattext texttype="text/html">&lt;i&gt;&lt;span&gt;&amp;nbsp;You would never finish!&lt;/span&gt;&lt;/i&gt;</mattext>
            </material>
          </flow_mat>
        </itemfeedback>
        <itemfeedback ident="4838_fb">
          <flow_mat>
            <material>
              <mattext texttype="text/html">&lt;i&gt;&lt;span&gt;This would be O(n) over the number of nodes in the data structure, not O(log n).&lt;/span&gt;&lt;/i&gt;</mattext>
            </material>
          </flow_mat>
        </itemfeedback>
        <itemfeedback ident="3794_fb">
          <flow_mat>
            <material>
              <mattext texttype="text/html">&lt;i&gt;&lt;span&gt;Constant time is O(1), not O(log n).&lt;/span&gt;&lt;/i&gt;</mattext>
            </material>
          </flow_mat>
        </itemfeedback>
      </item>
