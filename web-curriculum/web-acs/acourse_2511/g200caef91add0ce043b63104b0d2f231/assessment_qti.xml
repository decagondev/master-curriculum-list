      <item ident="g6d4f78f3ffd6b8732b737cc9c44015a8" title="Question">
        <itemmetadata>
          <qtimetadata>
            <qtimetadatafield>
              <fieldlabel>cc_profile</fieldlabel>
              <fieldentry>cc.multiple_choice.v0p1</fieldentry>
            </qtimetadatafield>
          </qtimetadata>
        </itemmetadata>
        <presentation>
          <material>
            <mattext texttype="text/html">&lt;div&gt;&lt;span&gt;In the relationship between Binary Trees and Binary Search Trees&lt;/span&gt;&lt;/div&gt;</mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="9809">
                <material>
                  <mattext texttype="text/plain">Every Binary Search Tree is a Binary Tree</mattext>
                </material>
              </response_label>
              <response_label ident="5533">
                <material>
                  <mattext texttype="text/plain">Every Binary Tree is a Binary Search Tree</mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar maxvalue="100" minvalue="0" varname="SCORE" vartype="Decimal"/>
          </outcomes>
          <respcondition continue="Yes">
            <conditionvar>
              <varequal respident="response1">5533</varequal>
            </conditionvar>
            <displayfeedback feedbacktype="Response" linkrefid="5533_fb"/>
          </respcondition>
          <respcondition continue="No">
            <conditionvar>
              <varequal respident="response1">9809</varequal>
            </conditionvar>
            <setvar action="Set" varname="SCORE">100</setvar>
          </respcondition>
        </resprocessing>
        <itemfeedback ident="5533_fb">
          <flow_mat>
            <material>
              <mattext texttype="text/html">&lt;i&gt;&lt;span&gt;&amp;nbsp;Binary Search Trees are binary trees with specific requirements about the relative ordering of nodes. Binary trees that do not meet this requirement are not Binary Search Trees.&lt;/span&gt;&lt;/i&gt;</mattext>
            </material>
          </flow_mat>
        </itemfeedback>
      </item>
      <item ident="gf6f27d4dd96c0f81f582814d37b44697" title="Question">
        <itemmetadata>
          <qtimetadata>
            <qtimetadatafield>
              <fieldlabel>cc_profile</fieldlabel>
              <fieldentry>cc.multiple_choice.v0p1</fieldentry>
            </qtimetadatafield>
          </qtimetadata>
        </itemmetadata>
        <presentation>
          <material>
            <mattext texttype="text/html">&lt;div&gt;&lt;span&gt;What are the characteristics of a binary tree?&lt;/span&gt;&lt;/div&gt;</mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="6700">
                <material>
                  <mattext texttype="text/plain">Every node leads to up to two other nodes.</mattext>
                </material>
              </response_label>
              <response_label ident="9798">
                <material>
                  <mattext texttype="text/plain">All nodes left of a node have values less than that node.</mattext>
                </material>
              </response_label>
              <response_label ident="9752">
                <material>
                  <mattext texttype="text/plain">All nodes left of a node have values greater than that node.</mattext>
                </material>
              </response_label>
              <response_label ident="5463">
                <material>
                  <mattext texttype="text/plain">Every node leads to up to three other nodes</mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar maxvalue="100" minvalue="0" varname="SCORE" vartype="Decimal"/>
          </outcomes>
          <respcondition continue="Yes">
            <conditionvar>
              <varequal respident="response1">9798</varequal>
            </conditionvar>
            <displayfeedback feedbacktype="Response" linkrefid="9798_fb"/>
          </respcondition>
          <respcondition continue="Yes">
            <conditionvar>
              <varequal respident="response1">9752</varequal>
            </conditionvar>
            <displayfeedback feedbacktype="Response" linkrefid="9752_fb"/>
          </respcondition>
          <respcondition continue="Yes">
            <conditionvar>
              <varequal respident="response1">5463</varequal>
            </conditionvar>
            <displayfeedback feedbacktype="Response" linkrefid="5463_fb"/>
          </respcondition>
          <respcondition continue="No">
            <conditionvar>
              <varequal respident="response1">6700</varequal>
            </conditionvar>
            <setvar action="Set" varname="SCORE">100</setvar>
          </respcondition>
        </resprocessing>
        <itemfeedback ident="9798_fb">
          <flow_mat>
            <material>
              <mattext texttype="text/html">&lt;i&gt;&lt;span&gt;This is a characteristic of a Binary Search Tree, but not necessarily of a Binary Tree.&lt;/span&gt;&lt;/i&gt;</mattext>
            </material>
          </flow_mat>
        </itemfeedback>
        <itemfeedback ident="9752_fb">
          <flow_mat>
            <material>
              <mattext texttype="text/html">&lt;i&gt;&lt;span&gt;This is a characteristic of a Binary Search Tree, but not necessarily of a Binary Tree.&lt;/span&gt;&lt;/i&gt;</mattext>
