        <presentation>
          <material>
            <mattext texttype="text/html">&lt;div&gt;&lt;p&gt;&lt;span&gt;What would the following log to the console: &lt;/span&gt;&lt;/p&gt;
&lt;p&gt;&lt;code class="js language-js"&gt;1 if ('hello') {&lt;/code&gt;&lt;/p&gt;
&lt;p&gt;&lt;code class="js language-js"&gt; 2 console.log('hello');}&lt;/code&gt;&lt;/p&gt;
&lt;p&gt;&lt;code class="js language-js"&gt;3 else { &lt;/code&gt;&lt;/p&gt;
&lt;p&gt;&lt;code class="js language-js"&gt; 4 console.log('goodbye');&lt;/code&gt;&lt;/p&gt;
&lt;p&gt;&lt;code class="js language-js"&gt;  5 } &lt;/code&gt;&lt;/p&gt;&lt;/div&gt;</mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="8461">
                <material>
                  <mattext texttype="text/plain">hello</mattext>
                </material>
              </response_label>
              <response_label ident="6118">
                <material>
                  <mattext texttype="text/plain">goodbye</mattext>
                </material>
              </response_label>
              <response_label ident="2410">
                <material>
                  <mattext texttype="text/plain">Nothing would be logged to the console</mattext>
                </material>
              </response_label>
              <response_label ident="4608">
                <material>
                  <mattext texttype="text/plain">An error would be thrown</mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
