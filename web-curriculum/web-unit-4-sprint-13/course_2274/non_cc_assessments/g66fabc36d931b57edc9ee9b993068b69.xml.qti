    <item ident="gec7e45ab59cf0e28ad14cfefc09101b7" title="c0633158-7e8d-43b2-b5e9-eb929a12e324">
      <itemmetadata>
        <qtimetadata>
          <qtimetadatafield>
            <fieldlabel>question_type</fieldlabel>
            <fieldentry>multiple_choice_question</fieldentry>
          </qtimetadatafield>
          <qtimetadatafield>
