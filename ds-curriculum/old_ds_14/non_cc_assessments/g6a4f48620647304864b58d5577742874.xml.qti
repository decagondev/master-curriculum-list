      </itemmetadata>
      <presentation>
        <material>
          <mattext texttype="text/html">&lt;div&gt;If we have a model with a hidden layer of 250 nodes, and output layer of 10 nodes, and train the model for 25 epochs, how many times will the weights be adjusted during the training?&lt;/div&gt;</mattext>
        </material>
        <response_lid ident="response1" rcardinality="Single">
          <render_choice>
            <response_label ident="5735">
              <material>
                <mattext texttype="text/plain">25</mattext>
              </material>
            </response_label>
            <response_label ident="9214">
              <material>
                <mattext texttype="text/plain">250</mattext>
              </material>
            </response_label>
            <response_label ident="4339">
              <material>
                <mattext texttype="text/plain">10</mattext>
              </material>
            </response_label>
            <response_label ident="5094">
              <material>
                <mattext texttype="text/plain">250 * 10 = 2500</mattext>
              </material>
            </response_label>
          </render_choice>
        </response_lid>
      </presentation>
      <resprocessing>
        <outcomes>
          <decvar maxvalue="100" minvalue="0" varname="SCORE" vartype="Decimal"/>
