    <item ident="geaddb30605826e7d15cbdb62919bbdfb" title="7a6c6ec8-4892-4e5a-8e4c-0e612a3af126">
      <itemmetadata>
        <qtimetadata>
          <qtimetadatafield>
            <fieldlabel>question_type</fieldlabel>
            <fieldentry>multiple_choice_question</fieldentry>
          </qtimetadatafield>
