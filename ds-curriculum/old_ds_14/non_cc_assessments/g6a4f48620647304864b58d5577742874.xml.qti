      <presentation>
        <material>
          <mattext texttype="text/html">&lt;div&gt;What is a disadvantage of using a large learning rate?&lt;/div&gt;</mattext>
        </material>
        <response_lid ident="response1" rcardinality="Single">
          <render_choice>
            <response_label ident="5918">
              <material>
                <mattext texttype="text/plain">There might not be convergence toward a minimum of the loss function.</mattext>
              </material>
            </response_label>
            <response_label ident="1431">
              <material>
                <mattext texttype="text/plain">The neural network will return the result too quickly.</mattext>
              </material>
            </response_label>
            <response_label ident="1372">
              <material>
                <mattext texttype="text/html">A large learning rate doesn't work well when used with a network containing hidden layers.</mattext>
              </material>
            </response_label>
            <response_label ident="8538">
              <material>
                <mattext texttype="text/plain">A large learning rate limits the number of epochs that can be run.</mattext>
              </material>
            </response_label>
          </render_choice>
        </response_lid>
      </presentation>
      <resprocessing>
        <outcomes>
          <decvar maxvalue="100" minvalue="0" varname="SCORE" vartype="Decimal"/>
