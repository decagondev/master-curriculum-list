        <presentation>
          <material>
            <mattext texttype="text/html">&lt;div&gt;What is the reason for applying gradient descent to a neural network?&lt;/div&gt;</mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="113">
                <material>
                  <mattext texttype="text/plain">It is a technique for finding the parameter values (weights and biases) that minimize the loss function (i.e. minimize model error).</mattext>
                </material>
              </response_label>
              <response_label ident="7486">
                <material>
                  <mattext texttype="text/plain">To determine the ideal batch size for training a neural network.</mattext>
                </material>
              </response_label>
              <response_label ident="6941">
                <material>
                  <mattext texttype="text/plain">To determine the ideal number of epochs for which to train a neural network model.</mattext>
                </material>
              </response_label>
              <response_label ident="701">
                <material>
                  <mattext texttype="text/plain">To find the optimization function that best suits the data set.</mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar maxvalue="100" minvalue="0" varname="SCORE" vartype="Decimal"/>
          </outcomes>
          <respcondition continue="No">
            <conditionvar>
              <varequal respident="response1">113</varequal>
            </conditionvar>
            <setvar action="Set" varname="SCORE">100</setvar>
          </respcondition>
        </resprocessing>
      </item>
