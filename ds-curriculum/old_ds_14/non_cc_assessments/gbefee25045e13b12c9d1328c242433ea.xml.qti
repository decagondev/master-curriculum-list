    <item ident="geed4dcab59289f1c5656e21f165042db" title="4e3c6683-3e9d-42be-b7ad-b1e02c2da445">
      <itemmetadata>
        <qtimetadata>
          <qtimetadatafield>
            <fieldlabel>question_type</fieldlabel>
            <fieldentry>multiple_choice_question</fieldentry>
          </qtimetadatafield>
